{
  "name": "sienna-react",
  "homepage": "https://trauerfeier.online",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.9.0",
    "react-helmet": "^6.0.0",
    "react-jotform-embed": "^1.0.4",
    "react-router-dom": "^5.0.1",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot && cp build/index.html build/404.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "react-snapshot": "^1.3.0"
  },
  "reactSnapshot": {
    "include": [
      "/",
      "/more-info",
      "/press",
      "/forms/choose-a-moderator",
      "/forms/about-the-ceremony",
      "/forms/guest-list",
      "/imprint",
      "/terms",
      "/privacy"
    ],
    "exclude": [],
    "snapshotDelay": 50
  }
}
